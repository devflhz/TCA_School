@foreach (var item in Student.Phones.Where(a => a.StudentId == Student.StudentId).Select(a => a))
{
    <a href="tel:+@item.PhoneLink">@item.Phone1</a>
    <br />
}
<button @onclick="Close" class="btn btn-secondary">Close</button>

@code {
    [CascadingParameter] BlazoredModalInstance ModalInstance { get; set; }
    [Parameter] public Student Student { get; set; }

    void Close()
    {
        ModalInstance.CloseAsync();
    }
}
